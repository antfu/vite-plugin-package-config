    "@antfu/utils": "^0.1.4",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.41",
    "bumpp": "^6.0.6",
    "eslint": "^7.24.0",
    "eslint-plugin-jest": "^24.3.5",
    "esno": "^0.5.0",
    "tsup": "^4.12.5",
    "typescript": "^4.2.4",
    "vite": "^2.2.1"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  }
