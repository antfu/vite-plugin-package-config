    "lint": "eslint \"{src,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest"
  },
  "dependencies": {
    "debug": "^4.3.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.11.1",
    "@antfu/ni": "^0.11.0",
    "@antfu/utils": "^0.3.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.11",
    "bumpp": "^7.1.1",
    "eslint": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "esno": "^0.12.1",
    "tsup": "^5.10.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  }
